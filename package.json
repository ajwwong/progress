        "@hello-pangea/dnd": "^17.0.0",
        "@mantine/core": "^7.13.4",
        "@mantine/dates": "^7.13.4",
        "@mantine/hooks": "^7.13.4",
        "@mantine/notifications": "^7.13.4",
        "@medplum/core": "3.2.32",
        "@medplum/fhirtypes": "3.2.32",
        "@medplum/mock": "3.2.32",
        "@medplum/react": "3.2.32",
        "@medplum/react-hooks": "3.2.32",

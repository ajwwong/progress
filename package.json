{
    "name": "medplum-hello-world",
    "version": "3.2.18",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite --host",
        "lint": "eslint .",
        "preview": "vite preview",
        "test": "vitest",
        "test:watch": "vitest watch",

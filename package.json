        "@types/node": "^20.0.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "18.3.1",
        "@vitejs/plugin-react": "4.3.3",
        "esbuild": "^0.25.1",
        "fast-glob": "^3.3.3",
